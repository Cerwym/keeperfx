                "${env:DEVKITPRO}/portlibs/switch/include",
                "${env:DEVKITA64}/aarch64-none-elf/include"
            ],
            "defines": [
                "PLATFORM_SWITCH=1",
                "USE_PRE_FILE=1",
                "BFDEBUG_LEVEL=0",
                "__SWITCH__"
            ],
            "compilerPath": "${env:DEVKITA64}/bin/aarch64-none-elf-gcc",
            "cStandard": "gnu11",
            "cppStandard": "gnu++14",
