        {
            "name": "PS Vita (vitasdk)",
            "includePath": [
                "${workspaceFolder}/src",
                "${env:VITASDK}/arm-vita-eabi/include",
                "${env:VITASDK}/arm-vita-eabi/include/SDL2"
            ],
            "defines": [
                "PLATFORM_VITA=1",
                "USE_PRE_FILE=1",
                "BFDEBUG_LEVEL=0",
                "__vita__",
                "SCE_LIBC_USE_STDC"
            ],
            "compilerPath": "${env:VITASDK}/bin/arm-vita-eabi-gcc",
            "cStandard": "gnu11",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-arm"
        },
        {
            "name": "Nintendo 3DS (devkitARM)",
            "includePath": [
                "${workspaceFolder}/src",
                "${env:DEVKITPRO}/libctru/include",
                "${env:DEVKITPRO}/portlibs/3ds/include",
                "${env:DEVKITARM}/arm-none-eabi/include"
            ],
            "defines": [
                "PLATFORM_3DS=1",
                "USE_PRE_FILE=1",
                "BFDEBUG_LEVEL=0",
                "ARM11",
                "_3DS"
