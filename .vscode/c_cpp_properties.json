{
    "configurations": [
        {
            "name": "WSL MinGW",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/deps/zlib/include",
                "${workspaceFolder}/deps/spng/include",
                "${workspaceFolder}/sdl/include",
                "${workspaceFolder}/sdl/include/SDL2",
